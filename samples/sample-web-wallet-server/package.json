        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-prefer-arrow-functions": "3.6.2",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-unused-imports": "2.0.0",
        "jest": "29.7.0",
        "rimraf": "6.0.1",
        "rollup": "4.40.0",
        "ts-jest": "29.3.2",
        "ts-node-dev": "2.0.0",
        "typescript": "5.8.3",
        "chokidar": "4.0.3",
        "json-schema-to-ts": "3.1.1",
        "pino": "9.6.0",
        "pino-pretty": "13.0.0",
