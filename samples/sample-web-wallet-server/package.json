{
    "name": "@velocitycareerlabs/sample-web-wallet-server",
    "version": "0.5.0-build",
    "description": "Sample VNF Wallet Nodejs Server",
    "repository": "https://github.com/velocitycareerlabs/velocitycore",
    "scripts": {
        "lint": "eslint --ext .js,.ts,.tsx src",
        "lint:fix": "eslint --fix --ext .js,.ts,.tsx src",
        "test": "NODE_ENV=test node --test --test-concurrency=1 --experimental-test-module-mocks --experimental-test-coverage --test-reporter=spec --test-reporter-destination=stdout 'test/**/*.test.js'",
    "test:ci": "NODE_ENV=test node --test --test-concurrency=1 --experimental-test-module-mocks --experimental-test-coverage --test-reporter=spec --test-reporter=junit --test-reporter-destination=stdout --test-reporter-destination=test-results.junit.xml 'test/**/*.test.js'",
        "build": "tsc",
        "prepublishOnly": "tsc",
        "start": "node ./dist/Server.js",
        "start:dev": "ts-node --log-error --files src/Server.ts"

    },
    "license": "Apache-2.0",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">= 20.0.0"
    },
    "dependencies": {
        "@fastify/autoload": "^6.0.0",
        "@fastify/cors": "^11.0.0",
        "axios": "^1.7.9",
        "env-var": "^7.5.0",
        "dotenv": "^16.4.7",
        "fastify": "^5.0.0",
        "fastify-plugin": "^5.0.0",
        "@velocitycareerlabs/vc-checks": "0.5.0-build",
        "@velocitycareerlabs/vnf-nodejs-wallet-sdk": "0.5.0-build"
    },
    "devDependencies": {
        "@jest/globals": "29.7.0",
        "@rollup/plugin-node-resolve": "16.0.1",
        "@rollup/plugin-typescript": "12.1.2",
        "@types/jest": "29.5.14",
        "@types/node": "22.15.17",
        "eslint": "8.57.1",
        "eslint-config-prettier": "8.10.0",
        "eslint-import-resolver-typescript": "3.10.1",
        "eslint-plugin-autofix": "1.1.0",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-prefer-arrow-functions": "3.6.2",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-unused-imports": "2.0.0",
        "expect": "29.7.0",
        "ts-jest": "29.3.2",
        "ts-node": "10.9.2",
        "typescript": "5.8.3",
        "json-schema-to-ts": "3.1.1",
        "pino": "9.6.0",
        "pino-pretty": "13.0.0"
    },
    "nx": {
        "targets": {
            "build": {
                "outputs": [
                    "{projectRoot}/dist"
                ]
            }
        }
    }
}
