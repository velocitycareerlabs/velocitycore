{
  "name": "@velocitycareerlabs/sample-web-wallet-app",
  "version": "0.5.0-build",
  "description": "Sample VNF Web Wallet App",
  "repository": "https://github.com/velocitycareerlabs/velocitycore",
  "license": "Apache-2.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@types/node": "22.15.21",
    "@types/react": "18.3.22",
    "@types/react-dom": "18.3.7",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@vitejs/plugin-react-swc": "3.9.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.0",
    "eslint-import-resolver-typescript": "3.10.1",
    "eslint-plugin-autofix": "1.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prefer-arrow-functions": "3.6.2",
    "eslint-plugin-prettier": "4.2.1",
    "expect": "29.7.0",
    "global-jsdom": "26.0.0",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-tsconfig-paths": "5.1.4",
    "vite-plugin-svgr": "4.3.0"
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts,.tsx src",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx src",
    "lint:ci": "eslint --ext .js,.ts,.tsx --format json >> eslint-results.json src",
    "start": "vite",
    "build": "tsc & vite build",
    "serve": "vite preview",
    "test": "node --enable-source-maps --import=global-jsdom/register --import=tsx --import=../../packages/tests-helpers/src/setup-react-tests.mjs --test --test-reporter=spec --test-reporter-destination=stdout '**/*.test.ts' '**/*.test.tsx'",
    "test:ci": "node --enable-source-maps --import=global-jsdom/register --import=tsx --import=../../packages/tests-helpers/src/setup-react-tests.mjs  --test --experimental-test-coverage --test-reporter=spec --test-reporter-destination=stdout --test-reporter=lcov --test-reporter-destination=lcov.info '**/*.test.ts' '**/*.test.tsx'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
