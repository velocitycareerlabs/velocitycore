{
  "name": "@velocitycareerlabs/aws-clients",
  "version": "0.5.0-build",
  "description": "Set of aws functions and utils used by Velocity Foundation projects",
  "repository": "https://github.com/velocitycareerlabs/velocitycore",
  "main": "index.js",
  "author": "Denis Smolonski",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "NODE_ENV=test node --test --test-concurrency=1 --experimental-test-module-mocks --experimental-test-coverage --test-reporter=spec --test-reporter-destination=stdout 'test/**/*.test.js'",
    "test:ci": "NODE_ENV=test node --test --test-concurrency=1 --experimental-test-module-mocks --experimental-test-coverage --test-coverage-include='src/**' --test-reporter=spec --test-reporter=junit --test-reporter-destination=stdout --test-reporter-destination=test-results.junit.xml --test-reporter=lcov --test-reporter-destination=lcov.info 'test/**/*.test.js'",
    "lint": "eslint --ext .js .",
    "lint:ci": "eslint . --format json >> eslint-results.json",
    "lint:fix": "eslint --fix --ext .js ."
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.731.1",
    "@aws-sdk/client-kms": "^3.731.1",
    "@aws-sdk/client-s3": "^3.731.1",
    "@aws-sdk/client-ses": "^3.731.1",
    "@aws-sdk/client-sesv2": "^3.731.1",
    "@aws-sdk/client-sns": "^3.731.1",
    "@aws-sdk/lib-dynamodb": "^3.731.1",
    "@aws-sdk/s3-request-presigner": "^3.731.1",
    "fastify-plugin": "^5.0.0",
    "lodash": "^4.17.21",
    "twilio": "^5.0.0"
  },
  "devDependencies": {
    "@velocitycareerlabs/crypto": "0.5.0-build",
    "@velocitycareerlabs/jwt": "0.5.0-build",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-autofix": "1.1.0",
    "eslint-plugin-better-mutation": "1.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prefer-arrow-functions": "3.6.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-watch": "7.0.0",
    "expect": "29.7.0",
    "prettier": "2.8.8"
  },
  "nx": {
    "tags": ["lib"]
  }
}
