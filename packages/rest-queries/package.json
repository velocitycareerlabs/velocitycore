    "eslint-plugin-better-mutation": "1.7.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prefer-arrow-functions": "3.6.2",
    "eslint-plugin-prettier": "4.2.5",
    "eslint-watch": "7.0.0",
    "expect": "29.7.0",
    "prettier": "2.8.8"
